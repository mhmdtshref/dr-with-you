{
  "name": "dr-with-you",
  "version": "1.0.0",
  "description": "An app that will help hospitals during the COVID-19 crisis.",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "tsc": "tsc",
    "start": "npm run serve:production",
    "serve:local": "SET NODE_ENV=local && ts-node-dev --respawn --transpileOnly ./src/index.ts",
    "serve:development": "SET NODE_ENV=development && ts-node-dev --respawn --transpileOnly --env=development ./src/index.ts",
    "serve:production": "SET NODE_ENV=production && ts-node-dev --respawn --transpileOnly --env=production ./src/index.ts",
    "eslint": "eslint ./src/**/*",
    "eslint:fix": "eslint ./src/**/* --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhmdtshref/dr-with-you.git"
  },
  "keywords": [
    "covid-19",
    "crisis",
    "coronavirus",
    "doctor",
    "hospital",
    "app",
    "mobile",
    "backend"
  ],
  "author": "Mohamed Sharif",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mhmdtshref/dr-with-you/issues"
  },
  "homepage": "https://github.com/mhmdtshref/dr-with-you#readme",
  "dependencies": {
    "dotenv-flow": "^3.1.0",
    "express": "^4.17.1",
    "pg": "^8.0.2",
    "sequelize": "^5.21.6"
  },
  "devDependencies": {
    "@types/dotenv-flow": "^3.0.0",
    "@types/express": "^4.17.6",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "eslint": "^6.8.0",
    "eslint-config-google": "^0.14.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.8.3"
  }
}
